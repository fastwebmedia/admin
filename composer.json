{
    "name": "fastwebmedia/admin",
    "description": "Admin interface builder for Laravel.",
    "homepage": "http://www.fastwebmedia.com",
    "keywords": ["laravel", "admin", "administrator"],
	"license": "MIT",
	"authors": [
		{
			"name": "Fast Web Media",
			"email": "technical@fastwebmedia.com"
		}
	],
	"require": {
      "php": ">=5.4.0",
      "illuminate/support": "5.*",
      "doctrine/dbal": "~2.3",
      "laracasts/flash": "1.*",
      "laracasts/utilities": "~2.0",
      "daylerees/sanitizer": "~1.0",
      "fastwebmedia/laravel-roles": "1.*"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"orchestra/testbench": "3.*"
	},
	"autoload": {
		"psr-0": {
			"FWM\\Admin": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestBase.php",
			"tests/models"
		]
	}
}
